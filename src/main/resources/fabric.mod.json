{
  "schemaVersion": 1,
  "id": "bedrockexperience",
  "name": "Bedrock Experience",
  "version": "${version}",
  "description": "Bringing awesome features from Bedrock Edition to Java.",
  "authors": [
    "LambdAurora"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/bedrockexperience",
    "sources": "https://github.com/LambdAurora/BedrockExperience.git",
    "issues": "https://github.com/LambdAurora/BedrockExperience/issues"
  },
  "license": "MIT",
  "icon": "assets/bedrockexperience/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "me.lambdaurora.bedrockexperience.BedrockExperience"
